                                       "output":  {
                                                      "model_dir":  "artifacts/models/rl_real"
                                                  }
                                   }
                    },
    "backtest":  {
                     "command":  [
                                     "freqtrade",
                                     "backtesting",
                                     "--config",
                                     "freqtrade/user_data/config_freqai.json",
                                     "--strategy",
                                     "ExpressionLongStrategy",
                                     "--strategy-path",
                                     "freqtrade/user_data/strategies",
                                     "--timerange",
                                     "20210101-20211231",
                                     "--freqaimodel",
                                     "LightGBMRegressor"
                                 ],
                     "results_dir":  "freqtrade/user_data/backtest_results"
                 }
