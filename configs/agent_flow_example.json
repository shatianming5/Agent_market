{
    "feature":  {
                    "args":  [
                                 "--config",
                                 "user_data/config_freqai.json",
                                 "--output",
                                 "user_data/freqai_features_real.json"
                             ],
                    "cwd":  "freqtrade",
                    "script":  "scripts/freqai_feature_agent.py"
                },
    "expression":  {
                       "args":  [
                                    "--config",
                                    "user_data/config_freqai.json",
                                    "--output",
                                    "user_data/freqai_expressions.json",
                                    "--timeframe",
                                    "1h",
                                    "--llm-enabled",
                                    "--llm-base-url",
                                    "https://api.zhizengzeng.com/v1",
                                    "--llm-model",
                                    "glm-4-air",
                                    "--llm-count",
                                    "20",
                                    "--llm-retries",
                                    "3",
                                    "--llm-timeout",
                                    "60",
                                    "--llm-fallback"
                                ],
                       "feedback_top":  8,
                       "cwd":  "freqtrade",
                       "script":  "scripts/freqai_expression_agent.py"
                   },
    "ml_training":  {
                        "configs":  [
                                        {
                                            "data":  {
                                                         "feature_file":  "freqtrade/user_data/freqai_features_real.json",
                                                         "data_dir":  "freqtrade/user_data/data",
