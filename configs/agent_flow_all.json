{
  "feature": {
    "script": "freqtrade/scripts/freqai_feature_agent.py",
    "args": [
      "--config", "configs/config_freqai_multi.json",
      "--output", "user_data/freqai_features_multi.json",
      "--timeframe", "4h",
      "--pairs", "BTC/USDT", "ETH/USDT", "SOL/USDT", "ADA/USDT"
    ]
  },
  "expression": {
    "script": "freqtrade/scripts/freqai_expression_agent.py",
    "args": [
      "--config", "configs/config_freqai_multi.json",
      "--feature-file", "../user_data/freqai_features_multi.json",
      "--output", "user_data/freqai_expressions.json",
      "--timeframe", "4h",
      "--llm-model", "gpt-3.5-turbo",
      "--llm-count", "24",
      "--llm-loops", "1",
      "--llm-timeout", "60"
    ],
    "feedback_top": 0
  },
  "ml_training": {
    "config": {
      "data": {
        "feature_file": "user_data/freqai_features_multi.json",
        "data_dir": "freqtrade/user_data/data",
        "exchange": "binanceus",
        "pairs": ["BTC/USDT", "ETH/USDT"],
        "timeframe": "4h",
        "label_period": 18
      },
      "model": {
        "name": "lightgbm",
        "params": {
          "objective": "regression",
          "metric": "rmse",
          "learning_rate": 0.04,
          "num_leaves": 63,
          "feature_fraction": 0.8,
          "subsample": 0.85,
          "num_boost_round": 220,
          "model_dir": "artifacts/models/lightgbm_flow"
        }
      },
      "training": { "validation_ratio": 0.25 },
      "output": { "model_dir": "artifacts/models/lightgbm_flow" }
    }
  },
  "backtest": {
    "command": [
      "freqtrade", "backtesting",
      "--config", "configs/config_freqai_multi.json",
      "--strategy", "ExpressionLongStrategy",
      "--strategy-path", "freqtrade/user_data/strategies",
      "--datadir", "freqtrade/user_data/data",
      "--dataformat-ohlcv", "feather",
      "--timerange", "20210101-20211231",
      "--freqaimodel", "LightGBMRegressor",
      "--export", "trades",
      "--export-filename", "user_data/backtest_results/latest_trades_multi"
    ],
    "results_dir": "user_data/backtest_results/multi_4h",
    "feedback_path": "user_data/llm_feedback/latest_backtest_summary.json"
  }
}
